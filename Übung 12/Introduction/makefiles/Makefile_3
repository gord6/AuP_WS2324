# Variablen
CC = gcc
CFLAGS=-std=c99 -Wall -Wpedantic -Werror -g

# Regel f체r die Erstellung der ausf체hrbaren Datei
main: myfunction.o main.o
	$(CC) $^ -o $@
	
# Musterregel f체r die Erstellung der Objektdateien
%.o: %.c
	$(CC) $(CFLAGS) -c $< -o $@

# Regel zum Aufr채umen der generierten Dateien
clean:
	rm -f *.o main
